{
  "name": "battleship-2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:create": "db-migrate db:create battleship_development --config ./db/development.json -m ./db/migrations",
    "db:drop": "db-migrate db:drop battleship_development --config ./db/development.json -m ./db/migrations",
    "db:setup": "npm run db:drop && npm run db:create && npm run db:migrate",
    "db:reset": "db-migrate reset --config ./db/development.json",
    "db:migrate": "db-migrate up --config ./db/development.json -m ./db/migrations --force-exit",
    "db:migrate:dry": "db-migrate up --dry-run --config ./db/development.json -m ./db/migrations",
    "db:migrate:create": "db-migrate create --config ./db/development.json -m ./db/migrations",
    "dev": "npm run dev:server & npm run dev:client",
    "dev:client": "vue-cli-service serve --host localhost --port 8080",
    "dev:server": "nodemon ./server.js localhost 3000",
    "prod:serve": "vue-cli-service serve --host localhost",
    "prod:build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.2.2",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^8.2.0",
    "elliptic": "^6.5.3",
    "node-sass": "^4.14.1",
    "node-url-shortener": "^1.1.10",
    "nodemon": "^2.0.4",
    "pg": "^8.3.2",
    "pg-format": "^1.0.4",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11",
    "vue-unicons": "^2.3.0",
    "websocket-extensions": "^0.1.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-service": "^4.5.4",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^6.2.2",
    "jquery": "^3.5.1",
    "serialize-javascript": "^3.1.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
